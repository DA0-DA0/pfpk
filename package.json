{
  "name": "pfpk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "vitest --passWithNoTests",
    "build": "tsc",
    "dev": "wrangler dev --port 58229",
    "deploy": "wrangler deploy --minify",
    "log": "wrangler tail",
    "db:migrate": "wrangler d1 migrations apply pfpk",
    "db:migrate:local": "npm run db:migrate -- --local",
    "db:seed": "wrangler d1 execute pfpk --file=./schema.sql",
    "db:seed:local": "npm run db:seed -- --local",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "generate:types": "wrangler types"
  },
  "devDependencies": {
    "@chain-registry/types": "^0.50.1",
    "@cloudflare/vitest-pool-workers": "^0.8.57",
    "@types/crypto-js": "^4.1.1",
    "@types/secp256k1": "^4.0.6",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.26.0"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@cosmjs/amino": "^0.34.0",
    "@cosmjs/cosmwasm-stargate": "^0.34.0",
    "@cosmjs/crypto": "^0.34.0",
    "@cosmjs/encoding": "^0.34.0",
    "@tsndr/cloudflare-worker-jwt": "^3.2.0",
    "chain-registry": "^1.69.4",
    "crypto-js": "^4.1.1",
    "graphql": "^16.8.1",
    "itty-router": "^5.0.18",
    "react": "^18.2.0",
    "secp256k1": "^5.0.0"
  }
}
